# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Application Configuration
APP_NAME="Pharo Reviewer Agent API"
APP_VERSION="1.0.0"
DEBUG=false

# Gemini Model Configuration
MODEL_ID="gemini-3-pro-preview"

# Pharo MCP Server Configuration
PHARO_SERVER_URL="http://localhost:8086"
PHARO_MCP_SERVER_COMMAND="uv"
PHARO_MCP_SERVER_CWD="/path/to/your/pharo-smalltalk-interop-mcp-server"
PHARO_MCP_TIMEOUT=30

# Agent Configuration
MAX_VALIDATION_ITERATIONS=3

# CORS Configuration
# CORS_ORIGINS can be "*" or comma-separated origins like "http://localhost:3000,https://example.com"
CORS_ALLOW_CREDENTIALS=true
